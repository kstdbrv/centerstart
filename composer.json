{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
              "name": "kenwheeler/slick",
              "version": "1.8.0",
              "type": "drupal-library",
              "dist": {
                "url": "https://github.com/kenwheeler/slick/archive/1.8.0.zip",
                "type": "zip"
              }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/adminimal_admin_toolbar": "^1.11",
        "drupal/adminimal_theme": "^1.6",
        "drupal/anchor_link": "^2.5",
        "drupal/antibot": "^1.4",
        "drupal/blazy": "^2.1",
        "drupal/captcha": "^1.1",
        "drupal/ckeditor_font": "^1.1",
        "drupal/ckeditor_iframe": "^2.1",
        "drupal/ckeditor_templates": "^1.2",
        "drupal/ckeditor_youtube": "^1.3",
        "drupal/colorbox": "^1.7",
        "drupal/colorbutton": "^1.2",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "^9.1",
        "drupal/devel": "^4.1",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/easy_breadcrumb": "^1.15",
        "drupal/flexslider": "^2.0",
        "drupal/imce": "^2.3",
        "drupal/libraries": "^3.0@beta",
        "drupal/panelbutton": "^1.3",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/recaptcha": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/sharethis": "^2.0@beta",
        "drupal/slick": "^2.2",
        "drupal/slick_views": "^2.3",
        "drupal/superfish": "^1.4",
        "drupal/twig_tweak": "^3.0",
        "drupal/views_infinite_scroll": "1.7",
        "drush/drush": "^10.4",
        "kenwheeler/slick": "^1.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ]
        }
    }
}
